programa {
    funcao inicio() {
        inteiro dolar_cotation 
        inteiro amount_of_dolars
        
        inteiro amount_in_reais
        
        leia(dolar_cotation)
        leia(amount_of_dolars)
        
        amount_in_reais = dolar_cotation * amount_of_dolars
        
        escreva(amount_in_reais)
    }
}